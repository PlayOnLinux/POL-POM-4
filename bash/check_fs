# This is the startup script

[ "$PLAYONLINUX" = "" ] && exit 0
source "$PLAYONLINUX/lib/sources" 

[ "$POL_OS" = "Mac" ] && exit 0
# This script tests filesystem
# Usage :

# check_gl [arch]
# Arch = (x86 | amd64)

PartInfo="$(POL_System_PartInfo "$POL_USER_ROOT")"

echo $PartInfo
exit 0
